{
  "name": "GoalToLife",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "main": "dist/apps/service/main.js",
  "scripts": {
    "ng": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "e2e:app-e2e:watch": "ng e2e app-e2e --watch",
    "nx": "nx",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "dep-graph": "nx dep-graph",
    "workspace-schematic": "nx workspace-schematic",
    "firebase-deploy": "firebase deploy",
    "deploy:functions": "npm run firebase-deploy -- --only functions",
    "deploy:app": "npm run firebase-deploy -- --only hosting:productivity-app",
    "help": "nx help",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "prepare": "husky install",
    "test:pr": "npm run affected:test -- --parallel",
    "service:serve": "concurrently --kill-others \"npm run build -- service --source-map --watch\" \"firebase serve --only functions\"",
    "service:debug": "concurrently --kill-others \"npm run build -- service --source-map --watch\" \"firebase emulators:start --only functions --inspect-functions \"",
    "stop:emulator": "lsof -t -i:8081 -i:8085 | xargs kill -9",
    "firebase:local": "nx run-many --target build --all && npm run stop:emulator && firebase emulators:start --import e2e-seed",
    "e2e:pr": "nx run-many --target build --all && npm run stop:emulator && firebase emulators:exec --import e2e-seed 'nx e2e app-e2e --headless'",
    "e2e:ci": "nx run-many --target build --all && firebase emulators:exec --import e2e-seed 'nx e2e app-e2e --headless'",
    "codecov": "codecov -t "
  },
  "private": true,
